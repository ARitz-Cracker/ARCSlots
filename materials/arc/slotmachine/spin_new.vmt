"UnlitGeneric"
{
	"$basetexture" "arc/slotmachine/spin_new"
	
	"$translucent" 1

	"$scale" "[1 0.1.25]"
	"$center" "[0.0 0.0]"
	"Proxies"
	{
            	"LinearRamp"
            	{
                	"rate" -0.35
                	"initialValue" 0.0
                	"resultVar" "$center[1]"
            	}
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" -.07
			"texturescrollangle" -90.00
		}
        	"TextureTransform"
        	{
			"centerVar" "$center"
			"scaleVar" "$scale"
        		"resultVar" "$basetexturetransform"
        	}

	}
}
